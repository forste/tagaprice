<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:my="urn:import:org.tagaprice.client.gwt.client.generics.widgets" 
	>
	<g:VerticalPanel>
		<g:HTML>
			<h1>Edit / Add shop</h1>
		</g:HTML>
		<g:DecoratorPanel>
			<g:HTMLPanel>
				<table>
					<tr>
						<td>
							<g:Label >id</g:Label>
						</td>
						<td>
							<g:Label ui:field="id">id</g:Label>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="nameI18N">Name</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="name"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="streetI18N">Street</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="street"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="zipI18N">zip</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="zip"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="cityI18N">city</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="city"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="countryI18N">country</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="country"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="latI18N">lat</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="lat"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="lngI18N">lng</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="lng"></g:TextBox>
						</td>
					</tr>
				
					<tr>
						<td colspan="2">
							<g:Button ui:field="cancelButton">cancel</g:Button>
							<g:Button ui:field="saveButton">save shop</g:Button>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>

		</g:DecoratorPanel>
	</g:VerticalPanel>
</ui:UiBinder>
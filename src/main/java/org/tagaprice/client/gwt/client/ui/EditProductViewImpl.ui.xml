<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:HTML>
			<h1>TagAPrice Productmanagement - Edit / Add Product</h1>
		</g:HTML>
		<g:DecoratorPanel>
			<g:HTMLPanel>

				<table>
					<tr>
						<td>
							<g:Label>id</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="id"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label>Name</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="name"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label>Price</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="price"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label>Description</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="description"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label>Category</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="category"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td colspan="2">
							<g:Button ui:field="cancelButton">cancel</g:Button>
							<g:Button ui:field="saveButton">save product</g:Button>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>

		</g:DecoratorPanel>
	</g:VerticalPanel>
</ui:UiBinder>